{
  "name": "maaii-identity-access-mgmt",
  "version": "0.1.0",
  "description": "Identity Access Management",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "dev": "nodemon --exec babel-node -- src",
    "doc": "apidoc -i src/ -f \".*\\.js$\"",
    "lint": "eslint src",
    "test": "mocha",
    "test:coverage": "nyc --reporter=html --reporter=lcov --reporter=text mocha",
    "jenkins": "npm prune && npm i --verbose && npm run build",
    "jenkins:commit": "npm run jenkins && npm run lint && npm run test:coverage",
    "jenkins:merge": "npm run jenkins && npm run doc",
    "build-src": "babel -d ./build ./src -s --copy-files",
    "build": "npm run clean && npm run build-src",
    "server": "node ./build/index.js"
  },
  "repository": {
    "type": "git",
    "url": "gerrit.dev.maaii.com:29418/maaii-identity-access-mgmt"
  },
  "keywords": [
    "m800",
    "iam",
    "access",
    "identity",
    "permission"
  ],
  "author": "M800 Web Team",
  "license": "ISC",
  "dependencies": {
    "acl": "^0.4.9",
    "bcrypt": "^0.8.7",
    "body-parser": "^1.15.2",
    "bottlejs": "^1.3.0",
    "co-express": "^1.2.1",
    "common-errors": "^1.0.0",
    "express": "^4.14.0",
    "express-validator": "^2.20.8",
    "fast-json-patch": "^1.0.0",
    "gridfs-stream": "^1.1.1",
    "joi": "^9.0.1",
    "json-loader": "^0.5.4",
    "lodash": "^4.13.1",
    "m800-health-check": "^0.3.2",
    "method-override": "^2.3.6",
    "mime": "^1.3.4",
    "mongoose": "^4.5.3",
    "mongoose-timestamp": "^0.6.0",
    "multer": "^1.1.0",
    "nconf": "^0.8.4",
    "oidc-provider": "^0.7.1",
    "q": "^1.4.1",
    "tv4": "^1.2.7",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "apidoc": "^0.16.1",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.11.0",
    "chai": "^3.5.0",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.10.2",
    "eslint-plugin-jsx-a11y": "^1.5.5",
    "eslint-plugin-react": "^5.2.2",
    "mocha": "^2.5.3",
    "mockgoose": "^6.0.3",
    "nodemon": "^1.9.2",
    "nyc": "^7.0.0",
    "rimraf": "^2.5.3",
    "supertest": "^1.2.0"
  }
}
