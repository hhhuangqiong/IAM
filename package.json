{
  "name": "maaii-identity-access-mgmt",
  "version": "1.2.0",
  "description": "Identity Access Management",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf build public/assets/app doc",
    "dev": "babel-node -- src",
    "dev:api": "nodemon --exec \"babel-node -- src \" --ignore build",
    "docs:api": "m800-swagger-docs-generator -i src/api/swagger.yaml -o build/docs/api",
    "docs": "gitbook install && gitbook build . ./build/docs/book && npm run docs:api",
    "lint": "eslint src",
    "test": "cross-env NODE_ENV=test mocha",
    "coverage": "nyc npm run test",
    "jenkins": "npm prune && npm i --verbose && gulp intl:download && npm run build",
    "jenkins:commit": "npm run jenkins && npm run lint && npm run coverage",
    "jenkins:merge": "npm run jenkins && gulp intl:upload && npm run docs",
    "build:src": "babel -d ./build/src ./src -s --copy-files && webpack --config webpack/webpack.config.prod.babel.js",
    "build": "npm run clean && npm run build:src && npm run build:migration",
    "build:migration": "babel -d ./build/migrations ./migrations",
    "server": "node ./build/src/index.js",
    "start": "npm run server",
    "migrate": "cd build/migrations && migrate-mongo up && cd ../../"
  },
  "repository": {
    "type": "git",
    "url": "gerrit.dev.maaii.com:29418/maaii-identity-access-mgmt"
  },
  "keywords": [
    "m800",
    "iam",
    "access",
    "identity",
    "permission"
  ],
  "author": "M800 Web Team",
  "license": "ISC",
  "dependencies": {
